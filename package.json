{
  "name": "Template",
  "version": "1.0.0",
  "description": "React Template",
  "scripts": {
    "build": "babel-node express_src/build.js",
    "clean-dist": "rimraf ./dist && rimraf ./express_dist && mkdir dist",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "open:src": "babel-node express_src/srcServer.js",
    "prebuild": "npm-run-all clean-dist transpile-express #knex:migrate",
    "start": "npm-run-all test migrate && npm-run-all --parallel open:src lint:watch",
    "migrate": "#knex migrate:latest",
    "test": "mocha --reporter spec test/testHelper.js \"src/**/*.test.js\" \"test/express/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "transpile-express": "babel express_src -d express_dist"
  },
  "author": "Jerimiah Mair",
  "license": "MIT",
